//swapping of numbers without temp variable
#include <iostream>

using namespace std;

int main()
{
    cout<<"Hello World"<<endl;
    int a = 4, b = 5;
    cout<<"Value of a and b before swapping is : "<<a<<" "<<b<<endl;
    a = a*b;//20
    b = a/b;
    a = a/b;
    cout<<"Value of a and b after swapping is : "<<a<<" "<<b<<endl;
    return 0;
}
